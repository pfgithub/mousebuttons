event mouse.wheel (e) {
    if(mouse.button_6.held) {
        emit(keypress, .down, .VOLUMEUP);
	sync();
	emit(keypress, .up, .VOLUMEUP);
	return andSync();
    }
    emit(e);
    return andSync();
}

